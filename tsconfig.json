{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": true,
    "allowJs": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "inlineSourceMap": false,
    "inlineSources": false,
    "resolveJsonModule": true,
    "lib": ["es2019"]
  },
  "include": ["parsers"],
  "exclude": ["./dist", "parsers/**/*.spec.ts"]
}
